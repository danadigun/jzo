@{
    ViewData["Title"] = "Reset password confirmation";
}

<div class="password-confirmation animated slideInDown">
    <i class="ion ion-ios-checkmark-outline reset-success animated fadeIn"></i>
    <p class="alert alert-success ">
        <strong>@ViewData["Title"]   </strong>

        Your password has been reset. Please <a asp-controller="Account" asp-action="Login">Click here to log in</a>.
    </p>
</div>

